<?php

namespace Acme\Bundle\ProductBundle\Entity;

use Doctrine\ORM\Mapping as ORM;
use Acme\Bundle\ProductBundle\Entity\Serie;


class Product
{
    /**
     * @var int
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     */
    protected $serie; 

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return Product
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
    /**
     * @var \Acme\Bundle\ProductBundle\Entity\Serie
     */
    private $parent;


    /**
     * Set parent
     *
     * @param \Acme\Bundle\ProductBundle\Entity\Serie $parent
     * @return Product
     */
    public function setParent(\Acme\Bundle\ProductBundle\Entity\Serie $parent = null)
    {
        $this->parent = $parent;

        return $this;
    }

    /**
     * Get parent
     *
     * @return \Acme\Bundle\ProductBundle\Entity\Serie 
     */
    public function getParent()
    {
        return $this->parent;
    }
}
